<?xml version="1.0"?>
<!-- S_Reactor.xml -->
<scpd xmlns="urn:schemas-upnp-org:service-1-0">
	<specVersion>
		<major>1</major>
		<minor>0</minor>
	</specVersion>
	<serviceStateTable>
		<stateVariable sendEvents="yes">
			<name>Message</name>
			<dataType>string</dataType>
			<defaultValue></defaultValue>
			<shortCode>message</shortCode>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>NumChildren</name>
			<dataType>ui4</dataType>
			<defaultValue>0</defaultValue>
			<shortCode>children</shortCode>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>NumRunning</name>
			<dataType>ui4</dataType>
			<defaultValue>0</defaultValue>
			<shortCode>running</shortCode>
		</stateVariable>
		<stateVariable sendEvents="yes">
			<name>HouseMode</name>
			<dataType>ui4</dataType>
			<defaultValue>1</defaultValue>
		</stateVariable>
		<stateVariable sendEvents="yes">
			<name>LastDST</name>
			<dataType>boolean</dataType>
			<defaultValue></defaultValue>
			<shortCode>lastdst</shortCode>
		</stateVariable>
		<stateVariable sendEvents="yes">
			<name>SystemPowerSource</name>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable sendEvents="yes">
			<name>SystemBatteryLevel</name>
			<dataType>ui4</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_string</name>
			<dataType>string</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_boolean</name>
			<dataType>boolean</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_ui4</name>
			<dataType>ui4</dataType>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_SyslogFacility</name>
			<dataType>ui1</dataType>
			<defaultValue>23</defaultValue>
			<allowedValueRange>
				<minimum>0</minimum>
				<maximum>23</maximum>
			</allowedValueRange>
		</stateVariable>
		<stateVariable sendEvents="no">
			<name>A_ARG_TYPE_SyslogSeverity</name>
			<dataType>ui1</dataType>
			<defaultValue>5</defaultValue>
			<allowedValueRange>
				<minimum>0</minimum>
				<maximum>7</maximum>
			</allowedValueRange>
		</stateVariable>
	</serviceStateTable>
	<actionList>
		<action>
			<name>AddSensor</name>
			<argumentList>
				<argument>
				  <name>Count</name>
				  <direction>in</direction>
				  <relatedStateVariable>A_ARG_TYPE_ui4</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
		  <name>RunScene</name>
		  <argumentList>
			<argument>
			  <name>SceneNum</name>
			  <direction>in</direction>
			  <relatedStateVariable>A_ARG_TYPE_string</relatedStateVariable>
			</argument>
			<argument>
			  <name>Options</name>
			  <direction>in</direction>
			  <relatedStateVariable>A_ARG_TYPE_string</relatedStateVariable>
			</argument>
		  </argumentList>
		</action>
		<action>
		  <name>StopScene</name>
		  <argumentList>
			<argument>
			  <name>SceneNum</name>
			  <direction>in</direction>
			  <relatedStateVariable>A_ARG_TYPE_string</relatedStateVariable>
			</argument>
		  </argumentList>
		</action>
		<action>
			<name>UpdateGeofences</name>
			<argumentList>
				<argument>
					<name>event</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_string</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>MasterClear</name>
			<argumentList/>
		</action>
		<action>
			<name>RepairDevice</name>
			<argumentList>
				<argument>
				  <name>DeviceNum</name>
				  <direction>in</direction>
				  <relatedStateVariable>A_ARG_TYPE_ui4</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>SendSMTP</name>
			<argumentList>
				<argument>
					<name>From</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_string</relatedStateVariable>
				</argument>
				<argument>
					<name>To</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_string</relatedStateVariable>
				</argument>
				<argument>
					<name>Subject</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_string</relatedStateVariable>
				</argument>
				<argument>
					<name>Body</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_string</relatedStateVariable>
				</argument>
				<argument>
					<name>Cc</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_string</relatedStateVariable>
				</argument>
				<argument>
					<name>Bcc</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_string</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>SendSyslog</name>
			<argumentList>
				<argument>
					<name>Facility</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_SyslogFacility</relatedStateVariable>
				</argument>
				<argument>
					<name>Severity</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_SyslogSeverity</relatedStateVariable>
				</argument>
				<argument>
					<name>ServerIP</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_string</relatedStateVariable>
				</argument>
				<argument>
					<name>Application</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_string</relatedStateVariable>
				</argument>
				<argument>
					<name>Message</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_string</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>GetPluginVersion</name>
			<argumentList>
				<argument>
					<name>ResultVersion</name>
					<direction>out</direction>
					<relatedStateVariable>TempStorage</relatedStateVariable>
					<retval/>
				</argument>
			</argumentList>
		</action>
		<action>
			<name>SetDebug</name>
			<argumentList>
				<argument>
					<name>debug</name>
					<direction>in</direction>
					<relatedStateVariable>A_ARG_TYPE_boolean</relatedStateVariable>
				</argument>
			</argumentList>
		</action>
	</actionList>
</scpd>